{
  "version": 2,
  "builds": [
    {
      "src": "api/save-result.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/save-result",
      "methods": ["POST"],
      "dest": "/api/save-result.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
